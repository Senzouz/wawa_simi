        "        inputs, labels = data[0].to(device), data[1].to(device)\n",
        "        \n",
        "        optimizer.zero_grad()\n",
        "\n",
        "        inputs = inputs.unsqueeze(1)\n",
        "\n",
        "        # forward + backward + optimize\n",
        "        output = AlexNet_model1(inputs.float())\n",
        "        loss = criterion(output,labels)\n",
        "        loss.backward()\n",
        "        optimizer.step()\n",
