      "source": [
        "def spectrogram_image(y, sr):\n",
        "    mels = librosa.feature.mfcc(y=y, sr=sr, n_mfcc=13)\n",
        "    #mels = np.log(mels + 1e-9)\n",
        "\n",
        "    img = scale_minmax(mels, 0, 255).astype(np.uint8)\n",
        "    img = np.flip(img, axis=0)\n",
        "    img = 255-img\n",
        "\n",
        "    return img"
      ],
      "execution_count": null,
      "outputs": []
